 SELECT COUNT(*), 'Null' AS narrative 
  FROM refined_transactions 
 WHERE ADDRESS_ID IS NULL 
UNION
SELECT COUNT(*), 'Not Null' AS narrative 
  FROM refined_transactions 
 WHERE ADDRESS_ID IS NOT NULL;

